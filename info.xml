<?xml version="1.0"?>
<extension key="com.skvare.mosaicomigration" type="module">
  <file>mosaicomigration</file>
  <name>Mosaico Extension Directory Changes</name>
  <description>Mosaico Extension Directory Changes: Use it when the database is migrated from the production to the development environment and the extension directory path is changed.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Sunil Pawar</author>
    <email>sunil@skvare.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/Skvare/com.skvare.mosaicomigration</url>
    <url desc="Documentation">https://github.com/Skvare/com.skvare.mosaicomigration</url>
    <url desc="Support">https://github.com/Skvare/com.skvare.mosaicomigration</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-08-30</releaseDate>
  <version>1.4</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.51</ver>
  </compatibility>
  <requires>
    <ext version="~1.1">uk.co.vedaconsulting.mosaico</ext>
  </requires>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Mosaicomigration</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
